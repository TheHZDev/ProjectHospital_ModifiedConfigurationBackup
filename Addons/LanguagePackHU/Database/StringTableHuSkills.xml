            <GameDBLocalizedString>      <LocID>SKILL_LAB_SPECIALIST_SPEC_USG_DESCRIPTION</LocID>                   <Text>Az ultrahang specialisták működtetik az ultrahanggal működő eszközöket.</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>      <LocID>SKILL_LAB_SPECIALIST_SPEC_CARDIOLOGY</LocID>                        <Text>Kardiológus</Text>  </GameDBLocalizedString>
